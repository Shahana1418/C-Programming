#include<stdio.h>
#include<ctype.h>
#include<string.h>
int main()
{
    char str[100];
    int count=0;
    printf("enter the string:");
    scanf("%s",str);
    for(int i=0;str[i]!='\0';i++){
        char ch =tolower(str[i]);
        if(ch=='a'|| ch=='e'|| ch=='o'|| ch=='i'||ch=='u'){
            count++;
        }

    }
    printf("number of vowels:%d\n",count);
    return 0;
}
