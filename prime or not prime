#include<stdio.h>
int main()
{
    int number,i;
    printf("enter the number:");
    scanf("%d",&number);
    if(number<=1){
        printf("it is not a prime number");
       return 0; 
    }
    for(i=2;i<number;i++){
        if(number%i==0){
             printf("it is not a prime number");
       return 0; 
        }
    }
        printf("it is a prime number");
        return 0;
    
}